{
	"dir": "/usr/share/feuerland",
	"policy": {
		"input": "deny",
		"output": "accept"
	},
	"log": [
		"deny"
	],
	"rules": {
		"input": {
			"tcp": {
				"22": {
					"policy": "deny",
					"except": [ "rfc1918", "ipdeny/DE" ]
				},
				"53": {
					"policy": "accept"
				}
			},
			"udp": {
				"53": {
					"policy": "accept"
				}
			}
		}
	}
}
